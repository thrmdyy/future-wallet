{
  "manifest_version": 3,
  "name": "Crypto Wallet",
  "version": "0.0.1",
  "description": "Venom Crypto Wallet",
  "minimum_chrome_version": "102",
  "action": {
    "default_title": "Crypto Wallet",
    "default_popup": "index.html"
  },
  "icons": {
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "clipboardWrite",
    "activeTab",
    "notifications",
    "alarms"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}